{
  "name": "AdkSession",
  "version": "1",
  "desc": "Workflow for ADK session lifecycle management",
  "initialState": "initial_state",
  "active": true,
  "states": {
    "initial_state": {
      "transitions": [
        {
          "name": "activate",
          "next": "active",
          "manual": true
        }
      ]
    },
    "active": {
      "transitions": [
        {
          "name": "update",
          "next": "active",
          "manual": true
        },
        {
          "name": "archive",
          "next": "archived",
          "manual": true
        }
      ]
    },
    "archived": {
      "transitions": [
        {
          "name": "reactivate",
          "next": "active",
          "manual": true
        }
      ]
    }
  }
}
