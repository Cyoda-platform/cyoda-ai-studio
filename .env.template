CYODA_CLIENT_ID=""
CYODA_CLIENT_SECRET=""
CYODA_HOST=YOUR_ENV_NAME_VAR
CYODA_AI_API=cyoda
WORKFLOW_AI_API=workflow
CONNECTION_AI_API=connection
RANDOM_AI_API=random
TRINO_AI_API=trino
GRPC_PROCESSOR_TAG="cyoda_application"
CHAT_ID=CHAT_ID_VAR

# AI Model Configuration
# Supported models:
# - Google Gemini: gemini-2.0-flash-exp, gemini-2.5-pro-preview-03-25, gemini-2.5-flash
# - OpenAI via LiteLLM: openai/gpt-4o-mini, openai/gpt-4o
# - Anthropic via LiteLLM: anthropic/claude-3-haiku-20240307, anthropic/claude-3-5-sonnet-20241022
AI_MODEL=gemini-2.0-flash-exp

# Augment Code Model Configuration
# Model used for code generation with Augment CLI
# Supported models: haiku4.5, sonnet4, etc.
AUGMENT_MODEL=haiku4.5

# AI SDK Selection
# Choose which SDK to use for the AI Assistant:
# - "google" (default): Google ADK (Generative AI Development Kit)
# - "openai": OpenAI Agents SDK
AI_SDK=google

# Google ADK Configuration (when AI_SDK=google)
GOOGLE_API_KEY=
GOOGLE_MODEL=gemini-2.0-flash-exp
GOOGLE_TEMPERATURE=0.7
GOOGLE_MAX_TOKENS=8192

# OpenAI Configuration (when AI_SDK=openai)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=8192
# Enable debug logging for OpenAI SDK (set to "true" to enable)
DEBUG_OPENAI=false
